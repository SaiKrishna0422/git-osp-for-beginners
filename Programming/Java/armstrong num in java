class Armstrong{  
  public static void main(String[] args)  {  
    int a=0,b,t;  
    int n=153;  
    t=n;  
    while(n>0)  
    {  
    b=n%10;  
    n=n/10;  
    a=a+(b*b*b);  
    }  
    if(t==a)  
    System.out.println("armstrong number");   
    else  
        System.out.println("Not armstrong number");   
   }  
} 